{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Information",
        "System": "None",
        "Microsoft": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "NewsWebAPIContext": "NewsWebAPIContext"
  },

  "NewsAPI": {
    "Endpoints": {
      "BestStories": "https://hacker-news.firebaseio.com/v0/beststories.json",
      "StoryDetail": "https://hacker-news.firebaseio.com/v0/item/{0}.json"
    },
    "Database": {
      "StoreMinimum": 25,
      "StoreLimit": 50
    }
  }

}